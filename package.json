{
  "name": "supplement-recommendation-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prebuild": "expo prebuild && node scripts/setup-android.js",
    "prebuild:android": "expo prebuild --platform android && node scripts/setup-android.js",
    "prebuild:ios": "expo prebuild --platform ios",
    "build:android": "npm run prebuild:android && cd android && ./gradlew assembleRelease",
    "build:ios": "npm run prebuild:ios && cd ios && xcodebuild -workspace *.xcworkspace -scheme *.xcscheme -configuration Release archive"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/native-stack": "^6.11.0",
    "@tanstack/react-query": "^5.62.0",
    "axios": "^1.7.7",
    "expo": "~52.0.0",
    "expo-status-bar": "~2.0.0",
    "openai": "^6.9.1",
    "react": "18.3.1",
    "react-hook-form": "^7.53.0",
    "react-native": "0.76.5",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "~5.3.3"
  },
  "private": true
}
